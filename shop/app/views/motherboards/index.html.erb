<p id="notice"><%= notice %></p>

<h1>Motherboards</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Price</th>
      <th>Produced</th>
      <th>Brand</th>
      <th>Category</th>
      <th>Country</th>
      <th>Factor</th>
      <th>Soket</th>
      <th>Chipset</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @motherboards.each do |motherboard| %>
      <tr>
        <td><%= motherboard.name %></td>
        <td><%= motherboard.description %></td>
        <td><%= motherboard.price %></td>
        <td><%= motherboard.produced %></td>
        <td><%= motherboard.brand_id %></td>
        <td><%= motherboard.category_id %></td>
        <td><%= motherboard.country_id %></td>
        <td><%= motherboard.factor %></td>
        <td><%= motherboard.soket %></td>
        <td><%= motherboard.chipset %></td>
        <td><%= motherboard.type %></td>
        <td><%= link_to 'Show', motherboard %></td>
        <td><%= link_to 'Edit', edit_motherboard_path(motherboard) %></td>
        <td><%= link_to 'Destroy', motherboard, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Motherboard', new_motherboard_path %>
