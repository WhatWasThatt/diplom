<p id="notice"><%= notice %></p>

<h1>Processors</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Price</th>
      <th>Category</th>
      <th>Country</th>
      <th>Produced</th>
      <th>Brand</th>
      <th>Crystalcodename</th>
      <th>Socket</th>
      <th>Model</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @processors.each do |processor| %>
      <tr>
        <td><%= processor.name %></td>
        <td><%= processor.description %></td>
        <td><%= processor.price %></td>
        <td><%= processor.category %></td>
        <td><%= processor.country %></td>
        <td><%= processor.produced %></td>
        <td><%= processor.brand %></td>
        <td><%= processor.crystalcodename %></td>
        <td><%= processor.socket %></td>
        <td><%= processor.model %></td>
        <td><%= link_to 'Show', processor %></td>
        <td><%= link_to 'Edit', edit_processor_path(processor) %></td>
        <td><%= link_to 'Destroy', processor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Processor', new_processor_path %>
